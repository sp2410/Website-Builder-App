<%= render 'websites/verticaltab'%>

<script>
	$(document).ready(function() {
	  function setHeight() {
	    windowHeight = $(window).innerHeight();
	    $('.col-sm-2.sidenav.hidden-xs').css('min-height', windowHeight);
	  };
	  setHeight();
	  
	  $(window).resize(function() {
	    setHeight();
	  });
	});
</script>

